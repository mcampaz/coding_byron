<div class="row">
    {{#each vehiculos}}
        <div class="col-md-3">
            <div class="card">
                <div class="card-body">
                    <h3>{{manufacturer}}</h3>
                    <h3>{{model}}</h3>
                    <h3>{{model_year}}</h3>
                    <h3>{{vehicle_description}}</h3>
                    <h3>{{mileage}}</h3>
                    <h3>{{vehicle_condition}}</h3>
                </div>
                <div class="card-footer d-flex justify-content-between align-items-center">
                    <a href="/vehiculos/delete/{{_id}}" class="btn btn-danger">
                        Vender
                    </a>
                    <a href="/vehiculos/edit/{{_id}}" class="btn btn-secondary">
                        Editar
                    </a>
                </div>
            </div>
        </div>
    {{/each}}
</div>